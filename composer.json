{
    "name": "little-green-man/earhart",
    "description": "A package to help use PropelAuth with Laravel",
    "version": "1.5.0",
    "keywords": [
        "Little Green Man",
        "laravel",
        "Earhart",
        "PropelAuth"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/little-green-man/earhart",
    "authors": [
        {
            "name": "Elliot Ali",
            "email": "elliot@lgm.ltd"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/contracts": "^11.0||^12.0",
        "illuminate/support": "^11.0|^12.0",
        "socialiteproviders/propelauth": "^4.0",
        "svix/svix": "^1.81",
        "spatie/laravel-data": "^4.18"
    },
    "require-dev": {
        "larastan/larastan": "^2.0 || ^3.0",
        "laravel/pint": "^1.0",
        "pestphp/pest": "^1.23|^2.1|^3.1",
        "orchestra/testbench": "^9.15|^10.0"
    },
    "autoload": {
        "psr-4": {
            "LittleGreenMan\\Earhart\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LittleGreenMan\\Earhart\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LittleGreenMan\\Earhart\\ServiceProvider"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
